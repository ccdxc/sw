# Feature networking Modules List for Local to Remote scenario
feature:
    id          : networking
    sub         : local2remote
    package     : test.networking
    module      : networking
    runorder    : 100
    enable      : True
    ignore      : False

modules:
    # L2 switching based on MAC filtering
    # L2 switching within same subnet
    - module:
        name    : L2_IPV4_TCP_QTAG
        spec    : local2remote.testspec
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L2,Label=NETWORKING

    # L2 switching within same subnet
    - module:
        name    : L2_IPV6_TCP_QTAG
        spec    : local2remote.testspec
        ignore  : True
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV6,FwdMode=L2,Label=NETWORKING

    - module:
        name    : L3_IPV4_TCP_QTAG
        spec    : local2remote.testspec
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L3,Label=NETWORKING

    - module:
        name    : L3_IPV6_TCP_QTAG
        spec    : local2remote.testspec
        ignore  : True
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV6,FwdMode=L3,Label=NETWORKING

    - module:
        name    : L2_IPV4_TCP_QTAG_SRCGUARD_TRUE
        spec    : local2remote_srcguard_fail.testspec
        enable  : False
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L2,Label=NETWORKING

    - module:
        name    : L2_IPV4_TCP_QTAG_SRCGUARD_FALSE
        spec    : local2remote_srcguard_pass.testspec
        enable  : False
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L2,Label=NETWORKING

    - module:
        name    : L2_IPV6_TCP_QTAG_SRCGUARD_TRUE
        spec    : local2remote_srcguard_fail.testspec
        enable  : False
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV6,FwdMode=L2,Label=NETWORKING

    - module:
        name    : L2_IPV6_TCP_QTAG_SRCGUARD_FALSE
        spec    : local2remote_srcguard_pass.testspec
        enable  : False
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV6,FwdMode=L2,Label=NETWORKING

    - module:
        name    : L2_IPV4_ICMP_ECHO_QTAG
        spec    : local2remote_icmp_echo_request.testspec
        ignore  : True
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L2,Label=NETWORKING

    - module:
        name    : L2_IPV6_ICMP_ECHO_QTAG
        spec    : local2remote_icmp6_echo_request.testspec
        enable  : False
        ignore  : True
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV6,FwdMode=L2,Label=NETWORKING

    - module:
        name    : L2_IPV4_ICMP_ECHO_REPLY_QTAG
        spec    : local2remote_icmp_echo_reply.testspec
        ignore  : True
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L2,Label=NETWORKING

    - module:
        name    : L2_IPV6_ICMP_ECHO_REPLY_QTAG
        spec    : local2remote_icmp6_echo_reply.testspec
        enable  : False
        ignore  : True
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV6,FwdMode=L2,Label=NETWORKING

    - module:
        name    : L3_IPV4_ICMP_ECHO_QTAG
        spec    : local2remote_icmp_echo_request.testspec
        ignore  : True
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L3,Label=NETWORKING

    - module:
        name    : L3_IPV6_ICMP_ECHO_QTAG
        spec    : local2remote_icmp6_echo_request.testspec
        enable  : False
        ignore  : True
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV6,FwdMode=L3,Label=NETWORKING

    - module:
        name    : L3_IPV4_ICMP_ECHO_REPLY_QTAG
        spec    : local2remote_icmp_echo_reply.testspec
        ignore  : True
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L3,Label=NETWORKING

    - module:
        name    : L3_IPV6_ICMP_ECHO_REPLY_QTAG
        spec    : local2remote_icmp6_echo_reply.testspec
        enable  : False
        ignore  : True
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV6,FwdMode=L3,Label=NETWORKING

