# Test Spec
selectors:
    root: ref://store/templates/id=RDMA_SESSION
#    src:
#        tenant      : filter://overlay=VLAN,type=TENANT
#        segment     : filter://any
#        endpoint    : filter://remote=False
#        interface   : filter://any
#    dst:
#        tenant      : filter://overlay=VLAN,type=TENANT
#        segment     : filter://any
#        endpoint    : filter://remote=True
#        interface   : filter://any
#    flow            : filter://fwtype=L2,type=IPV4,dport=4791,proto=UDP,label=RDMA
    rdmasession     :
        lqp         : filter://svc=0
        rqp         : filter://svc=0
#    maxflows    : 1
#    maxrdmasessions     : 1

packets:
    - packet:
        id          : BASE_PKT1 # Base packet
        payloadsize : 64
        template    : ref://factory/packets/id=PAYLOAD
        headers     :
            payload:
                data    : pattern-random://0/255/1

    - packet:
        id          : BASE_PKT2 # Base packet
        payloadsize : 64
        template    : ref://factory/packets/id=PAYLOAD
        headers     :
            payload:
                data    : pattern-random://0/255/1

    - packet:
        id          : SEND_ONLY_1 # Base packet
        payloadsize : ref://testcase/packets/id=BASE_PKT1/size
        template    : callback://rdma/rdma/PickPktTemplate/v4=ETH_IPV4_TOS_UDP_BTH,v6=ETH_IPV6_UDP_BTH
        icrc        : True
        encaps      :
            - ref://factory/packets/id=ENCAP_QTAG
        headers     :
            # Add all possible headers, the final headers will be selected
            # based on the Flow.
            eth:
                src     : ref://testcase/config/rdmasession/session/initiator/ep/macaddr
                dst     : ref://testcase/config/rdmasession/session/responder/ep/macaddr
            qtag:
                vlan    : ref://testcase/config/rdmasession/session/initiator/ep/segment/vlan_id
            ipv4:
                src     : ref://testcase/config/rdmasession/session/initiator/addr
                dst     : ref://testcase/config/rdmasession/session/responder/addr
                ttl     : 64
            ipv6:
                src     : ref://testcase/config/rdmasession/session/initiator/addr
                dst     : ref://testcase/config/rdmasession/session/responder/addr
                hlim    : 64
            udp:
                sport   : ref://testcase/config/rdmasession/session/iflow/sport
                dport   : ref://testcase/config/rdmasession/session/iflow/dport
            bth:
                opcode  : 4 # RDMA SEND Only
                destqp  : ref://testcase/config/rdmasession/rqp/id
                psn     : callback://rdma/rdma/GetPktTxPsn/pkt_num=0
                a       : 1                
            payload:
                data    : ref://testcase/packets/id=BASE_PKT1/rawbytes

    - packet:
        id          : SEND_ONLY_2 # Base packet
        payloadsize : ref://testcase/packets/id=BASE_PKT2/size
        template    : callback://rdma/rdma/PickPktTemplate/v4=ETH_IPV4_TOS_UDP_BTH,v6=ETH_IPV6_UDP_BTH
        icrc        : True
        encaps      :
            - ref://factory/packets/id=ENCAP_QTAG
        headers     :
            # Add all possible headers, the final headers will be selected
            # based on the Flow.
            eth:
                src     : ref://testcase/config/rdmasession/session/initiator/ep/macaddr
                dst     : ref://testcase/config/rdmasession/session/responder/ep/macaddr
            qtag:
                vlan    : ref://testcase/config/rdmasession/session/initiator/ep/segment/vlan_id
            ipv4:
                src     : ref://testcase/config/rdmasession/session/initiator/addr
                dst     : ref://testcase/config/rdmasession/session/responder/addr
                ttl     : 64
            ipv6:
                src     : ref://testcase/config/rdmasession/session/initiator/addr
                dst     : ref://testcase/config/rdmasession/session/responder/addr
                hlim    : 64
            udp:
                sport   : ref://testcase/config/rdmasession/session/iflow/sport
                dport   : ref://testcase/config/rdmasession/session/iflow/dport
            bth:
                opcode  : 4 # RDMA SEND Only
                destqp  : ref://testcase/config/rdmasession/rqp/id
                psn     : callback://rdma/rdma/GetPktTxPsn/pkt_num=3
                a       : 1                
            payload:
                data    : ref://testcase/packets/id=BASE_PKT2/rawbytes

    - packet:
        id          : READ_REQ # Transmit packet
        payloadsize : 0
        template    : callback://rdma/rdma/PickPktTemplate/v4=ETH_IPV4_TOS_UDP_BTH_RETH,v6=ETH_IPV6_UDP_BTH_RETH
        icrc        : True
        encaps      :
            - ref://factory/packets/id=ENCAP_QTAG
        headers     :
            # Add all possible headers, the final headers will be selected
            # based on the Flow.
            eth:
                src     : ref://testcase/config/rdmasession/session/initiator/ep/macaddr
                dst     : ref://testcase/config/rdmasession/session/responder/ep/macaddr
            qtag:
                vlan    : ref://testcase/config/rdmasession/session/initiator/ep/segment/vlan_id
            ipv4:
                src     : ref://testcase/config/rdmasession/session/initiator/addr
                dst     : ref://testcase/config/rdmasession/session/responder/addr
                ttl     : 64
            ipv6:
                src     : ref://testcase/config/rdmasession/session/initiator/addr
                dst     : ref://testcase/config/rdmasession/session/responder/addr
                hlim    : 64
            udp:
                sport   : ref://testcase/config/rdmasession/session/iflow/sport
                dport   : ref://testcase/config/rdmasession/session/iflow/dport
            bth:
                opcode  : 12 # RDMA READ Request
                destqp  : ref://testcase/config/rdmasession/rqp/id
                psn     : callback://rdma/rdma/GetPktTxPsn/pkt_num=1
            reth:
                va      : ref://testcase/pvtdata/va
                r_key   : ref://testcase/pvtdata/r_key
                dma_len : ref://testcase/pvtdata/dma_len
            payload:
                data    : None

    - packet:
        id          : ACK_PKT # Base packet
        payloadsize : 0
        template    : callback://rdma/rdma/PickPktTemplate/v4=ETH_IPV4_TOS_UDP_BTH_AETH,v6=ETH_IPV6_UDP_BTH_AETH
        icrc        : True
        encaps      :
            - ref://factory/packets/id=ENCAP_QTAG
        headers     :
            # Add all possible headers, the final headers will be selected
            # based on the Flow.
             eth:
                 src     : ref://testcase/config/rdmasession/session/responder/ep/macaddr
                 dst     : ref://testcase/config/rdmasession/session/initiator/ep/macaddr
             qtag:
                 vlan    : ref://testcase/config/rdmasession/session/responder/ep/segment/vlan_id
             ipv4:
                 src     : ref://testcase/config/rdmasession/session/responder/addr
                 dst     : ref://testcase/config/rdmasession/session/initiator/addr
                 ttl     : 64
             ipv6:
                 src     : ref://testcase/config/rdmasession/session/responder/addr
                 dst     : ref://testcase/config/rdmasession/session/initiator/addr
                 hlim    : 64
             udp:
                 sport   : ref://testcase/config/rdmasession/session/iflow/sport
                 dport   : ref://testcase/config/rdmasession/session/iflow/dport
             bth:
                 opcode  : 17 # Acknowledge
                 destqp  : ref://testcase/config/rdmasession/lqp/id
                 psn     : callback://rdma/rdma/GetPktTxPsn/pkt_num=3
             aeth:
                syndrome : 14 # TODO - credits
                msn      : callback://rdma/rdma/GetPktMsn/ack_num=2
             payload:
                 data    : None

    - packet:
         id          : READ_RESP_FIRST_PYLD # Receive payload
         payloadsize : 1024
         template    : ref://factory/packets/id=PAYLOAD
         headers     :
             payload:
                 data    : pattern-random://0/255/1

    - packet:
         id          : READ_RESP_LAST_PYLD # Receive payload
         payloadsize : 64
         template    : ref://factory/packets/id=PAYLOAD
         headers     :
             payload:
                 data    : pattern-random://0/255/1

    - packet:
         id          : READ_RESP_FIRST    # Receive packet 
         payloadsize : ref://testcase/packets/id=READ_RESP_FIRST_PYLD/size
         template    : callback://rdma/rdma/PickPktTemplate/v4=ETH_IPV4_TOS_UDP_BTH_AETH,v6=ETH_IPV6_UDP_BTH_AETH
         icrc        : True
         encaps      :
             - ref://factory/packets/id=ENCAP_QTAG
         headers     :
             # Add all possible headers, the final headers will be selected
             # based on the Flow.
             eth:
                 src     : ref://testcase/config/rdmasession/session/responder/ep/macaddr
                 dst     : ref://testcase/config/rdmasession/session/initiator/ep/macaddr
             qtag:
                 vlan    : ref://testcase/config/rdmasession/session/responder/ep/segment/vlan_id
             ipv4:
                 src     : ref://testcase/config/rdmasession/session/responder/addr
                 dst     : ref://testcase/config/rdmasession/session/initiator/addr
                 ttl     : 64
             ipv6:
                 src     : ref://testcase/config/rdmasession/session/responder/addr
                 dst     : ref://testcase/config/rdmasession/session/initiator/addr
                 hlim    : 64
             udp:
                 sport   : ref://testcase/config/rdmasession/session/iflow/sport
                 dport   : ref://testcase/config/rdmasession/session/iflow/dport
             bth:
                 opcode  : 13
                 destqp  : ref://testcase/config/rdmasession/lqp/id
                 psn     : callback://rdma/rdma/GetPktTxPsn/pkt_num=1
             aeth:
                syndrome : 14 # TODO - credits
                msn      : callback://rdma/rdma/GetPktMsn/ack_num=2
             payload:
                 data    : ref://testcase/packets/id=READ_RESP_FIRST_PYLD/rawbytes

    - packet:
         id          : READ_RESP_LAST     # Receive packet 
         payloadsize : ref://testcase/packets/id=READ_RESP_LAST_PYLD/size
         template    : callback://rdma/rdma/PickPktTemplate/v4=ETH_IPV4_TOS_UDP_BTH_AETH,v6=ETH_IPV6_UDP_BTH_AETH
         icrc        : True
         encaps      :
             - ref://factory/packets/id=ENCAP_QTAG
         headers     :
             # Add all possible headers, the final headers will be selected
             # based on the Flow.
             eth:
                 src     : ref://testcase/config/rdmasession/session/responder/ep/macaddr
                 dst     : ref://testcase/config/rdmasession/session/initiator/ep/macaddr
             qtag:
                 vlan    : ref://testcase/config/rdmasession/session/responder/ep/segment/vlan_id
             ipv4:
                 src     : ref://testcase/config/rdmasession/session/responder/addr
                 dst     : ref://testcase/config/rdmasession/session/initiator/addr
                 ttl     : 64
             ipv6:
                 src     : ref://testcase/config/rdmasession/session/responder/addr
                 dst     : ref://testcase/config/rdmasession/session/initiator/addr
                 hlim    : 64
             udp:
                 sport   : ref://testcase/config/rdmasession/session/iflow/sport
                 dport   : ref://testcase/config/rdmasession/session/iflow/dport
             bth:
                 opcode  : 15
                 destqp  : ref://testcase/config/rdmasession/lqp/id
                 psn     : callback://rdma/rdma/GetPktTxPsn/pkt_num=2
             aeth:
                syndrome : 14 # TODO - credits
                msn      : callback://rdma/rdma/GetPktMsn/ack_num=2
             payload:
                 data    : ref://testcase/packets/id=READ_RESP_LAST_PYLD/rawbytes

buffers:
    - buffer:
        id      : BUF1
        template: ref://factory/templates/id=RDMA_BUFFER
        fields:
           slab    : ref://testcase/config/rdmasession/lqp/pd/ep/slabs/id=SLAB0000
           offset  : 0
           segments:
               - size    : ref://testcase/packets/id=BASE_PKT1/size
                 data    : ref://testcase/packets/id=BASE_PKT1/rawbytes

    - buffer:
        id      : BUF2
        template: ref://factory/templates/id=RDMA_BUFFER
        fields:
           slab    : ref://testcase/config/rdmasession/lqp/pd/ep/slabs/id=SLAB0001
           offset  : 64
           segments:
               - size    : ref://testcase/packets/id=READ_RESP_FIRST_PYLD/size
                 data    : None
               - size    : ref://testcase/packets/id=READ_RESP_LAST_PYLD/size
                 data    : None
    - buffer:
        id      : BUF3
        template: ref://factory/templates/id=RDMA_BUFFER
        fields:
           slab    : ref://testcase/config/rdmasession/lqp/pd/ep/slabs/id=SLAB0001
           offset  : 0
           segments:
               - size    : ref://testcase/packets/id=BASE_PKT2/size
                 data    : ref://testcase/packets/id=BASE_PKT2/rawbytes

    - buffer:
           id      : EXP_BUF
           template: ref://factory/templates/id=RDMA_BUFFER
           fields:
              slab    : None
              offset  : 0
              segments:
                  - size    : ref://testcase/packets/id=READ_RESP_FIRST_PYLD/size
                    data    : ref://testcase/packets/id=READ_RESP_FIRST_PYLD/rawbytes
                  - size    : ref://testcase/packets/id=READ_RESP_LAST_PYLD/size
                    data    : ref://testcase/packets/id=READ_RESP_LAST_PYLD/rawbytes

descriptors:
    - descriptor:
        id      : SQ_DESC1
        template: ref://factory/templates/id=DESC_RDMA_SQ
        fields:
            op_type : 0  # OP_TYPE_SEND
            wrid    : 6
            num_sges: 1

            send    :
                len    : ref://testcase/buffers/id=BUF1/size
               
            sges:
                - va  : ref://testcase/buffers/id=BUF1/address
                  len : ref://testcase/buffers/id=BUF1/size
                  l_key: ref://testcase/config/rdmasession/lqp/pd/mrs/id=MR-SLAB0000/lkey

    - descriptor:
        id      : SQ_DESC2
        template: ref://factory/templates/id=DESC_RDMA_SQ
        fields:
            op_type : 3  # OP_TYPE_READ
            wrid    : 7
            num_sges: 1

            read    :
                va    : ref://testcase/pvtdata/va
                len   : ref://testcase/pvtdata/dma_len
                r_key : ref://testcase/pvtdata/r_key

            sges:
                - va  : ref://testcase/buffers/id=BUF2/address
                  len : ref://testcase/buffers/id=BUF2/size
                  l_key: ref://testcase/config/rdmasession/lqp/pd/mrs/id=MR-SLAB0001/lkey

    - descriptor:
        id      : SQ_DESC3
        template: ref://factory/templates/id=DESC_RDMA_SQ
        fields:
            op_type : 0  # OP_TYPE_SEND
            wrid    : 8
            num_sges: 1

            send    :
                 len  : ref://testcase/buffers/id=BUF3/size
               
            sges:
                - va  : ref://testcase/buffers/id=BUF3/address
                  len : ref://testcase/buffers/id=BUF3/size
                  l_key: ref://testcase/config/rdmasession/lqp/pd/mrs/id=MR-SLAB0001/lkey

    - descriptor:
        id      : EXP_CQ_DESC_1
        template: ref://factory/templates/id=DESC_RDMA_CQ_SEND
        fields:
            msn     : callback://rdma/rdma/GetCQMsn/sq_wqe_num=0
            qid     : ref://testcase/config/rdmasession/lqp/id
            #op_type : 0
            status  : 0  #CQ_STATUS_SUCCESS
            color   : callback://rdma/rdma/GetReqRxCQExpColor/cq_wqe_num=0
            error   : 0
            type    : 2 #CQE_TYPE_SEND_MSN


    - descriptor:
        id      : EXP_CQ_DESC_2
        template: ref://factory/templates/id=DESC_RDMA_CQ_SEND
        fields:
            msn     : callback://rdma/rdma/GetCQMsn/sq_wqe_num=1
            qid     : ref://testcase/config/rdmasession/lqp/id
            #op_type : 3
            status  : 0  #CQ_STATUS_SUCCESS
            color   : callback://rdma/rdma/GetReqRxCQExpColor/cq_wqe_num=1
            error   : 0
            type    : 2 #CQE_TYPE_SEND_MSN

    - descriptor:
        id      : EXP_CQ_DESC_3
        template: ref://factory/templates/id=DESC_RDMA_CQ_SEND
        fields:
            msn     : callback://rdma/rdma/GetCQMsn/sq_wqe_num=2
            qid     : ref://testcase/config/rdmasession/lqp/id
            #op_type : 0
            status  : 0  #CQ_STATUS_SUCCESS
            color   : callback://rdma/rdma/GetReqRxCQExpColor/cq_wqe_num=2
            error   : 0
            type    : 2 #CQE_TYPE_SEND_MSN

session:
    - step:
        trigger:
            descriptors:
                - descriptor:
                    object  : ref://testcase/descriptors/id=SQ_DESC1
                    ring    : ref://testcase/config/rdmasession/lqp/sq/rings/id=SQ
            
                - descriptor:
                    object  : ref://testcase/descriptors/id=SQ_DESC2
                    ring    : ref://testcase/config/rdmasession/lqp/sq/rings/id=SQ

                - descriptor:
                    object  : ref://testcase/descriptors/id=SQ_DESC3
                    ring    : ref://testcase/config/rdmasession/lqp/sq/rings/id=SQ

            doorbell:
                object: ref://testcase/config/rdmasession/lqp/sq/rings/id=SQ/doorbell
                fields:
                    upd : 0x9 # PI from DB data and DB_SCHED_EVAL
       
        expect:
            packets:
                - packet:
                    object  : ref://testcase/packets/id=SEND_ONLY_1
                    port    : ref://testcase/config/rdmasession/session/responder/ep/intf/ports

                - packet:
                    object  : ref://testcase/packets/id=READ_REQ
                    port    : ref://testcase/config/rdmasession/session/responder/ep/intf/ports

                - packet:
                    object  : ref://testcase/packets/id=SEND_ONLY_2
                    port    : ref://testcase/config/rdmasession/session/responder/ep/intf/ports

    - step:
        trigger:
            packets:
                - packet:
                    object  : ref://testcase/packets/id=ACK_PKT
                    port    : ref://testcase/config/rdmasession/session/responder/ep/intf/ports

        expect:
            descriptors:
                - descriptor:
                    object  : ref://testcase/descriptors/id=EXP_CQ_DESC_1
                    ring    : ref://testcase/config/rdmasession/lqp/sq_cq/rings/id=CQ

            packets:
                - packet:
                    object  : ref://testcase/packets/id=READ_REQ
                    port    : ref://testcase/config/rdmasession/session/responder/ep/intf/ports

                - packet:
                    object  : ref://testcase/packets/id=SEND_ONLY_2
                    port    : ref://testcase/config/rdmasession/session/responder/ep/intf/ports

    - step:
        trigger:
            packets:
                - packet:
                    object  : ref://testcase/packets/id=READ_RESP_FIRST
                    port    : ref://testcase/config/rdmasession/session/responder/ep/intf/ports

                - packet:
                    object  : ref://testcase/packets/id=READ_RESP_LAST
                    port    : ref://testcase/config/rdmasession/session/responder/ep/intf/ports

        expect:
            descriptors:
                - descriptor:
                    object  : ref://testcase/descriptors/id=EXP_CQ_DESC_2
                    ring    : ref://testcase/config/rdmasession/lqp/sq_cq/rings/id=CQ

    - step:
        trigger:
            packets:
                - packet:
                    object  : ref://testcase/packets/id=ACK_PKT
                    port    : ref://testcase/config/rdmasession/session/responder/ep/intf/ports

        expect:
            descriptors:
                - descriptor:
                    object  : ref://testcase/descriptors/id=EXP_CQ_DESC_3
                    ring    : ref://testcase/config/rdmasession/lqp/sq_cq/rings/id=CQ

    #set CI for CQ ring to that of PI in DB data
    - step:

        trigger:
            doorbell:
                object: ref://testcase/config/rdmasession/lqp/sq_cq/rings/id=CQ/doorbell
                fields:
                    upd : 0x4 # Set CI to PI(DB data)

        expect:
