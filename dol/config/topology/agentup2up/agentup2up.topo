# Topology Specification
meta:
    id: TOPO_AGENT_UP2UP

uplink:
    - entry:
        id      : Uplink1
        port    : 1
        mode    : PC_MEMBER
        sriov   : True
        status  : UP

    - entry:
        id      : Uplink2
        port    : 2
        mode    : PC_MEMBER
        sriov   : True
        status  : UP

    - entry: 
        id      : Uplink3
        port    : 3
        mode    : PC_MEMBER
        sriov   : False
        status  : UP
        pc      : 1

    - entry:
        id      : Uplink4
        port    : 4
        mode    : PC_MEMBER
        sriov   : False
        status  : UP
        pc      : 1

uplinkpc:
    - entry:
        id      : UplinkPc1
        port    : 3
        mode    : TRUNK
        members :
            - ref://store/objects/id=Uplink1
            - ref://store/objects/id=Uplink2
            - ref://store/objects/id=Uplink3
            - ref://store/objects/id=Uplink4

acls: None

qos : True

security_profiles: ref://store/specs/id=SECURITY_PROFILES_ACTIVE

tenants:
    -   spec    : ref://store/specs/id=TENANT_AGENT_UP2UP
        count   : 1
        lifns   : None

cpu: None
