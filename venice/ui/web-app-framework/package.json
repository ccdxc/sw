{
  "name": "web-app-framework",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "test-prod": "ng test --prod --watch=false --source-map=false --browsers=ChromeHeadlessCI",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "packagr": "ng-packagr -p ng-package.json",
    "bundle-styles": "scss-bundle -c scss-bundle.config.json",
    "pack": "yarn run packagr && yarn run bundle-styles && tar cvf web-app-framework.tgz ./dist",
    "replaceShrinkwrap": "sed -i.bak 's/http:/https:/g' npm-shrinkwrap.json && rm npm-shrinkwrap.json.bak"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.0.3",
    "@angular/cdk": "^7.0.3",
    "@angular/common": "7.0.3",
    "@angular/compiler": "7.0.3",
    "@angular/core": "7.0.3",
    "@angular/flex-layout": "7.0.0-beta.19",
    "@angular/forms": "7.0.3",
    "@angular/http": "7.0.3",
    "@angular/material": "^7.0.3",
    "@angular/platform-browser": "7.0.3",
    "@angular/platform-browser-dynamic": "7.0.3",
    "@angular/router": "7.0.3",
    "ajv": "^6.4.0",
    "core-js": "^2.5.4",
    "d3": "^5.2.0",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.21",
    "material-design-icons": "^3.0.1",
    "plotly.js": "^1.35.2",
    "primeicons": "^1.0.0-beta.9",
    "primeng": "^6.1.5",
    "rxjs": "^6.3.3",
    "typescript": "3.1.6",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.10.4",
    "@angular/cli": "7.0.4",
    "@angular/compiler-cli": "7.0.3",
    "@angular/language-service": "7.0.3",
    "@types/d3": "^4.12.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~9.4.7",
    "codelyzer": "^4.5.0",
    "jasmine-core": "3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-bullet": "^1.0.3",
    "ng-packagr": "^5.1.0",
    "protractor": "~5.3.0",
    "scss-bundle": "^2.1.3",
    "ts-node": "~5.0.1",
    "tsickle": "^0.33.1",
    "tslint": "~5.9.1"
  }
}
