{
  "name": "venice-ui",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng-high-memory": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng",
    "build-prod": "yarn run ng-high-memory -- build --prod",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "dev": "yarn run ng-high-memory -- serve",
    "dev-container": "yarn run ng-high-memory -- serve --host=0.0.0.0",
    "test": "yarn run ng-high-memory -- test",
    "test-prod": "npm run ng-high-memory -- test --prod --watch=false --source-map=false --browsers=ChromeHeadlessCI",
    "lint": "npm run lint-css && npm run lint-html && ng lint --fix",
    "lint-ci": "npm run lint-css && npm run lint-html && ng lint",
    "lint-css": "find ./src -name '*.scss'  -not -path './src/assets/generated/*' | xargs npx css-beautify -r --config jsbeautify.css.json",
    "lint-html": "find ./src -name '*.html' -not -path './src/assets/generated/*' | xargs npx html-beautify -r --config jsbeautify.json",
    "svgo": "find ./dist/assets -name '*.svg' | xargs svgo",
    "e2e": "export DISABLE_FOCUS_TESTS='true' && ng e2e -s=false",
    "e2e-dev": "export DISABLE_FOCUS_TESTS='false' && ng e2e -s=false --proxy-config proxy.conf.js",
    "replaceShrinkwrap": "sed -i.bak 's/http:/https:/g' npm-shrinkwrap.json && rm npm-shrinkwrap.json.bak",
    "webdriver-update-ci": "webdriver-manager update --standalone false --gecko false --versions.chrome 2.44"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.0.3",
    "@angular/cdk": "^7.0.3",
    "@angular/common": "7.0.3",
    "@angular/compiler": "7.0.3",
    "@angular/core": "7.0.3",
    "@angular/flex-layout": "7.0.0-beta.19",
    "@angular/forms": "7.0.3",
    "@angular/http": "7.0.3",
    "@angular/material": "^7.0.3",
    "@angular/platform-browser": "7.0.3",
    "@angular/platform-browser-dynamic": "7.0.3",
    "@angular/router": "7.0.3",
    "@ng-idle/core": "^6.0.0-beta.4",
    "@ng-idle/keepalive": "^6.0.0-beta.3",
    "@ngrx/core": "^1.2.0",
    "@ngrx/effects": "^7.0.0-beta.0",
    "@ngrx/store": "^7.0.0-beta.0",
    "@types/chart.js": "^2.7.41",
    "@types/jquery": "^3.3.5",
    "@types/lodash": "^4.14.123",
    "ajv": "^6.4.0",
    "angular-gridster2": "^4.7.2",
    "angular2-moment": "^1.9.0",
    "angular2gridster": "^1.3.4",
    "bootstrap": "^4.1.3",
    "chart.js": "^2.7.2",
    "chartjs-plugin-datalabels": "^0.2.0",
    "core-js": "^2.5.4",
    "d3": "^5.2.0",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "jquery": "^3.3.1",
    "karma-verbose-reporter": "^0.0.6",
    "lodash": "^4.17.21",
    "lru_map": "^0.3.3",
    "material-design-icons": "^3.0.1",
    "moment": "^2.22.0",
    "ng2-charts": "^1.6.0",
    "oboe": "^2.1.4",
    "plotly.js": "^1.35.2",
    "pluralize": "^7.0.0",
    "popper.js": "^1.14.4",
    "primeicons": "^1.0.0-beta.9",
    "primeng": "^6.1.5",
    "rxjs": "^6.3.3",
    "rxjs-compat": "^6.3.3",
    "typeface-fira-sans-condensed": "0.0.54",
    "typeface-roboto-condensed": "0.0.54",
    "typescript": "3.1.6",
    "web-app-framework": "file:.yalc/web-app-framework",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular/cli": "^7.3.9",
    "@angular/compiler-cli": "7.0.3",
    "@angular/language-service": "7.0.3",
    "@types/d3": "4.12.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~9.4.7",
    "@types/oboe": "^2.0.28",
    "codelyzer": "^4.5.0",
    "jasmine-core": "3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "js-beautify": "^1.9.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-bullet": "^1.0.3",
    "protractor": "~5.3.0",
    "svgo": "^1.1.1",
    "ts-node": "~5.0.1",
    "tsconfig-paths": "^3.4.2",
    "tslint": "~5.9.1"
  }
}
